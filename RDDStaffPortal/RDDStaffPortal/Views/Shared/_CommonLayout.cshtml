        var t = true;
        var pass = "";
        var repass = '';
        var emailReg = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[@@$!%*?&])[A-Za-z\d@@$!%*?&]{7,}$/;
        $("input[id='idnpass'],input[id='idcpass']").blur(function () {
            debugger
            pass = $("input[id='idnpass']").val();
            repass = $('input[id=idcpass]').val();
            $("#idnpass").prev("span").remove();
            if (!emailReg.test(pass)) {            
                $("#idnpass").before('<span class="has-error">Minimum Seven characters, at least one uppercase letter, one lowercase letter, one number and one special character.</span>');
                t = true;
            }
            else if (pass != repass) {
                $('#idnpass').addClass('has-error');
                $('#idcpass').addClass('has-error');
                t = true;
            }
            else {
                t = false;
                $('#idnpass').removeClass('has-error').addClass('has-success');
                $('#idcpass').removeClass('has-error').addClass('has-success');
            }
        });

        $("#btnpass").on("click", function () {
            if (pass.length == 0) {
                RdotAlerterrtxt1("Please Enter Password");
                $("input[id='idnpass']").focus();
                return
            }
            if (repass.length == 0) {
                RdotAlerterrtxt1("Please Enter Confirm Password");
                $("input[id='idcpass']").focus();
                return
            }
            if (t == true) {
                RdotAlerterrtxt1("Password does not match");
                return
            }
            $.getJSON("/ChangePassword", { opass: $("#idopass").val(), npass: $("#idnpass").val(), cpass: $("#idcpass").val() }, function (response) {
                if (response.data.Success == true) {
                    $("#btncls").trigger("click");
                    $("input[type='password']").val('');
                    RdotAlertSucesstxt(response.data.Message);

                } else {
                    RdotAlerterrtxt1(response.data.Message);
                }
            })
        })
