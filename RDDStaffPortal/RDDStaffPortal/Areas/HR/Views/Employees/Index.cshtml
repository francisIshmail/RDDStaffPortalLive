    $('#AddCMRow').on('click', '.ibtnDel', function (event) {
        debugger;
        var counter = 0;
        var tr = $(this).closest(".reddotTableRow");
        var delid = tr.find("[id='EmpPrEdu_EId']").val();
        if (delid == "0") {
            $(this).closest(".reddotTableRow").remove();
            counter -= 1
        }
        else {

            const swalWithBootstrapButtons = Swal.mixin({
                confirmButtonClass: 'btn btn-success',
                cancelButtonClass: 'btn btn-danger',
                buttonsStyling: false,
            })
            swalWithBootstrapButtons.fire({
                title: 'Are you sure?',
                text: "You won't be able to revert this!",
                type: 'warning',
                showCancelButton: true,
                confirmButtonText: 'Yes, delete it!',
                cancelButtonText: 'No, cancel!',
                reverseButtons: true
            }).then((result) => {
                if (result.value) {
                    $.getJSON("/Employees/DeleteRecord", { EId: delid }).done(function (data) {

                        if (data.DeleteFlag == "Deleted Sucessfully.") {
                            tr.remove();
                            //$('#btnreload').trigger('click');
                        }
                    });
                    swalWithBootstrapButtons.fire(
                        'Deleted!',
                        'Your Educational Info has been deleted.',
                        'success'
                    )

                } else if (
                    // Read more about handling dismissals
                    result.dismiss === Swal.DismissReason.cancel
                ) {
                    swalWithBootstrapButtons.fire(
                        'Cancelled',
                        'Your Educational Info is safe :)',
                        'error'
                    )
                }
            })



        }


    });


