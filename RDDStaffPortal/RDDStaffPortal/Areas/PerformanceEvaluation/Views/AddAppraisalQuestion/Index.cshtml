                }
                $('.loader1').hide();
            }    
        });
    });

    $(document).on('click', '#btnDownloadExcel', function () {
        window.location.href = '@Url.Action("Download", "AddAppraisalQuestion")';
    });
    $(document).on('change', '#action_json', function () {
        var Files = $(this).get(0).files;
        if (Files[0].name != "") {
            $("#ddlCopyFromPrevious").attr("disabled", true);
            $("#action_name").attr("disabled", true);
            $("#add-more").attr("disabled", true);
            $("#ddlCopyFromPrevious").css("cursor", "not-allowed");
            $("#action_name").css("cursor", "not-allowed");
            $("#add-more").css("cursor", "not-allowed");
        }
        else {
            $("#ddlCopyFromPrevious").removeAttr("disabled", true);
            $("#action_name").removeAttr("disabled", true);
            $("#add-more").removeAttr("disabled", true);
            $("#action_name").css("cursor", "");
            $("#ddlCopyFromPrevious").css("cursor", "");
            $("#add-more").css("cursor", "");
        }
        var filedata = new FormData();
        filedata.append(Files[0].name, Files[0]);
        $.ajax({
            type: "POST",
            url: '@Url.Action("GetExcelData", "AddAppraisalQuestion")',
            data: filedata,
            contentType: false,
            processData: false,
            dataType: "json",
            success: function (data) {
                //$('#add-rowExcel').dataTable().fnDestroy();
                $("#add-rowExcel tbody").empty();
                $("#add-rowExcel").show();
                var Html = "";
                if (data.length > 0) {
                    $.each(data, function (Index, Value) {
                        Html += '<tr>';
                        Html += '<td>' + '<input type="text" class="form-control appqstn" value="' + Value.AppraisalQuestion + '">' + '</td>';
                        Html += '<td><div class="form-button-action"><button type="button" data-toggle="tooltip" title="" class="btn btn-link btn-danger delete btnDel" data-original-title="Remove"><i class="fa fa-times"></i></button></div></td>';
                        Html += '</tr>';
                    });
                }
                else {
                    Html += '<tr>';
                    Html += '<td colspan="6" style="text-align:center">No Data Found</td>';
                    Html += '</tr>';
                }
                $("#add-rowExcel tbody").append(Html);

            }
        });
    });
    $(document).on('change', '#ddlCopyFromPrevious', function () {
        var CategoryId = $("#ddlCategory option:selected").val();
        if (CategoryId == "-1") {
            RedDotAlert_Error("Please Select Category First");
            return false;
        }
        var PrevPeriod = $("#ddlCopyFromPrevious option:selected").val();
        
        if (PrevPeriod != '-1') {
            $("#action_json").attr("disabled", true);
            $("#action_name").attr("disabled", true);
            $("#add-more").attr("disabled", true);
            $("#action_name").css("cursor", "not-allowed");
            $("#action_json").css("cursor", "not-allowed");
            $("#add-more").css("cursor", "not-allowed");
            GetPreviousPeriodQuestion(PrevPeriod, CategoryId);
        }
        else {
            $("#add-rowPrevious").hide();
            $("#add-rowPrevious tbody").empty();
            $("#action_json").removeAttr("disabled", true);
            $("#action_name").removeAttr("disabled", true);
            $("#add-more").removeAttr("disabled", true);
            $("#action_name").css("cursor", "");
            $("#action_json").css("cursor", "");
            $("#add-more").css("cursor", "");
        }
        
    });

    $(document).on('change', '#ddlCategory', function () {
        var PrevPeriod = $("#ddlCopyFromPrevious option:selected").val();
        var CategoryId = $(this).val();     
        if (PrevPeriod == "-1") {
            //RedDotAlert_Error("Please Select Category First");
            return false;
        }
        else {
            $("#action_json").attr("disabled", true);
            $("#action_name").attr("disabled", true);
            $("#add-more").attr("disabled", true);
            $("#action_name").css("cursor", "not-allowed");
            $("#action_json").css("cursor", "not-allowed");
            $("#add-more").css("cursor", "not-allowed");
            GetPreviousPeriodQuestion(PrevPeriod, CategoryId);
        }       
    });
    $(document).on('click', '.btnDel', function () {
        $(this).parents("tr").remove();
    });
    $(document).on('keyup', '#action_name', function () {
        var Key = $(this).val();
        if (Key.length > 0 || $("#action_name").val()!="") {
            $("#action_json").attr("disabled", true);
            $("#ddlCopyFromPrevious").attr("disabled", true);
            $("#ddlCopyFromPrevious").css("cursor", "not-allowed");
            $("#action_json").css("cursor", "not-allowed");
        }
        else {
            $("#action_json").removeAttr("disabled", true);
            $("#ddlCopyFromPrevious").removeAttr("disabled", true);
            $("#ddlCopyFromPrevious").css("");
            $("#action_json").css("");
        }
    });
    $(document).on('click', '#btnSaveQuestion', function () {
        var Catid = $("#ddlCategory option:selected").val();
        var Period = $("#ddlPeriod option:selected").val();
        var QYear = $("#ddlYear option:selected").val();
        if (Catid == "-1") {
            RedDotAlert_Error("Please Select Category");
            return false;
        }
        if (Period == "-1") {
            RedDotAlert_Error("Please Select Period");
            return false;
        }
        if (QYear == "-1") {
            RedDotAlert_Error("Please Select Year");
            return false;
        }
        var rDD_Question = {
            CategoryId: Catid,
            Period: Period,
            Year: QYear,
            rDD_QuestionList:[]
        };
        if ($("#action_json").prop("disabled") == false) {
            $("#add-rowExcel tbody tr").each(function () {
                var qstn = $.trim($('.appqstn', this).val());
                var rDD_QuestionList = {
                    Question: qstn
                };
                rDD_Question.rDD_QuestionList.push(rDD_QuestionList);
            });
        }
        else if ($("#action_name").prop("disabled") == false) {
            $('.txtQuestion').each(function () {
                var qstn = $.trim($(this).val());
                var rDD_QuestionList = {
                    Question: qstn
                };
                rDD_Question.rDD_QuestionList.push(rDD_QuestionList);
            });
        }
        else if ($("#ddlCopyFromPrevious").prop("disabled") == false) {
            $("#add-rowPrevious tbody tr").each(function () {
                var qstn = $.trim($('.appqstns', this).val());
                var rDD_QuestionList = {
                    Question: qstn
                };
                rDD_Question.rDD_QuestionList.push(rDD_QuestionList);
            });
        }
        if ($("#action_json").prop("disabled") == false && $("#action_name").prop("disabled") == false && $("#ddlCopyFromPrevious").prop("disabled") == false) {
            RedDotAlert_Error("Please Select Anyone Option To Save Question");
            return false;
        }
        SaveQuestion(rDD_Question);
    });

    $(document).on('click', '.selectCategory', function () {
        var CategoryId = $(this).attr("data");
        var Qperiod = $("#ddlSelectYear option:selected").val();
        $("#tblShowQuestion tbody").empty();
        $("#tblShowQuestion tbody").append('<tr><td colspan="6" style="text-align:center">Please Wait....</td></tr>');        
        $.ajax({
            type: "POST",
            url: '@Url.Action("GetCategorywiseQuestionList", "AddAppraisalQuestion")',
            data: { "CategoryId": CategoryId, "Qperiod": Qperiod},
            dataType: "json",
            success: function (data) { 
                debugger
                var d = data.Table;
                var str = "";
                var Count = 0;
                $("#tblShowQuestion tbody").empty();                
                if (d.length > 0) {
                    $.each(d, function (Index, Value) {                        
                        str += '<tr>';
                        str += '<td style="display:none">' + Value.QuestionTransId + '</td>';
                        str += '<td>' + Value.Question + '</td>';                                              
                        str += '<td><div class="form-button-action"><button type="button" id="" title="" class= "btn btn-link btn-primary btn-lg edit dis" data-original-title="Edit Task"><i class="fa fa-edit"></i></button> <button type="button" id="" title="" class= "btn btn-link btn-success update dis" data-original-title="Edit Task"><i class="fas fa-save"></i></button> <button type="button" data-toggle="tooltip" title="" class="btn btn-link btn-danger delete dis" data-original-title="Remove"><i class="fa fa-times"></i></button></div></td>';
                        str += '</tr>';                        
                    });
                }
                else {
                    str += '<tr>';
                    str += '<td colspan="6" style="text-align:center">Question Not Assigned Under This Category</td>';                    
                    str += '</tr>';     
                }    
                $("#tblShowQuestion tbody").append(str);
                $(".update").attr("hidden", "hidden");
                if (data.Table1.length > 0) {
                    var LaunchStatus = data.Table1[0].AppraisalLaunchStatus;
                }
                if (LaunchStatus == true) {
                    $('.dis').prop("disabled", true);
                    $('.dis').css("cursor", "not-allowed");
                }
                else {
                    $('.dis').prop("disabled", false);
                    $('.dis').css("cursor", "");
                }
            }
        });
    });
    $(document).on('click', '.edit', function () {        
        var tr = $(this).closest("tr");        
        tr.find(".edit").attr("hidden", "hidden");
        tr.find(".delete").attr("hidden", "hidden");
        tr.find(".update").removeAttr("hidden", "hidden");       
        $(this).parents("tr").find("td:eq(1)").html('<input type="text"  class="form-control" id="txtQstn" value="' + $(this).parents("tr").find("td:eq(1)").html() + '">');
        //$(this).parents("tr").find("td:eq(2)").html('<div class="form-button-action"><button type="button" id="" title="" class= "btn btn-link btn-success save" data-original-title="Edit Task"><i class="fas fa-save"></i></button> <button type="button" data-toggle="tooltip" title="" class="btn btn-link btn-danger delete" data-original-title="Remove"><i class="fa fa-times"></i></button></div>')        
    });   
    $(document).on('click', '.update', function () {
        $('.loader1').show();
        var tr = $(this).closest("tr");
        var Qid = tr.find("td:eq(0)").html();       
        var Question = tr.find("#txtQstn").val();        
        $.ajax({
            type: "POST",
            url: '@Url.Action("UpdateAppraisalQuestion", "AddAppraisalQuestion")',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify ({ Qid: Qid, Question: Question }),
            dataType: "json",
            success: function (data) {
                debugger
                $('.loader1').hide();
                if (data[0].Outtf == true) {                    
                    RedDotAlert_Success(data[0].Responsemsg);
                    GetQuestionDetails();
                    tr.find(".edit").removeAttr("hidden", "hidden");
                    tr.find(".delete").removeAttr("hidden", "hidden");
                    tr.find(".update").attr("hidden", "hidden");
                }
                else {
                    RedDotAlert_Error(data[0].Responsemsg);
                }
            }
        });
    });
    $(document).on('click', '.delete', function () {
        var Qid = $(this).closest("tr").find("td:eq(0)").html();
        const swalWithBootstrapButtons = Swal.mixin({
            confirmButtonClass: 'btn btn-success',
            cancelButtonClass: 'btn btn-danger',
            buttonsStyling: false,
        })
        swalWithBootstrapButtons.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            type: 'warning',
            showCancelButton: true,
            confirmButtonText: 'Yes, delete it!',
            cancelButtonText: 'No, cancel!',
            reverseButtons: true
        }).then((result) => {            
            if (result.value) {
                $('.loader1').show();
                $.getJSON("/AddAppraisalQuestion/DeleteAppraisalQuestion", { "Qid": Qid }).done(function (data) {
                    $('.loader1').hide();
                    if (data[0].Outtf == true) {
                        RedDotAlert_Success(data[0].Responsemsg);
                        GetQuestionDetails();
                    }
                    else {
                        RedDotAlert_Error(data[0].Responsemsg);
                    }
                });
            }
            else if (
                // Read more about handling dismissals
                result.dismiss === Swal.DismissReason.cancel
            ) {
                swalWithBootstrapButtons.fire(
                    'Cancelled',
                    'Your Record  is safe :)',
                    'error'
                )
            }
            $('.loader1').hide();
        });
    });
</script>

