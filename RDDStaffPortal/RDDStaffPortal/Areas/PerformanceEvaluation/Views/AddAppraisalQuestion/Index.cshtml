<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<script src="~/Scripts/RedDotUtility.js"></script>

<script type="text/javascript">
    function GetCategoryList() {        
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetPeriodCategoryList", "AddAppraisalQuestion")',
            dataType: "json",
            success: function (data) {  
                debugger
                $("#ddlCopyFromPrevious").empty();
                $("#ddlCopyFromPrevious").append('<option value="-1">--Select--</option>');
                $.each(data.Table, function (index, Value) {
                    $("#ddlCopyFromPrevious").append("<option value='" + Value.PreviousPeriod + "'>" + Value.PreviousPeriod + "</option>");
                    
                });
                //$("#ddlCopyFromPrevious").append("<option value='H2-2020'>H2-2020</option>");
                $("#ddlCategory").empty();
                $("#ddlCategory").append('<option value="-1">--Select--</option>');
                $.each(data.Table1, function (index, Value) {
                    $("#ddlCategory").append("<option value='" + Value.CategoryId + "'>" + Value.CategoryName + "</option>");
                });
                $("#ddlPeriod").empty();
                $("#ddlPeriod").append('<option value="-1">--Select--</option>');
                $.each(data.Table2, function (index, Value) {
                    $("#ddlPeriod").append("<option value='" + Value.Period + "'>" + Value.Period + "</option>");
                });
                $("#ddlYear").empty();
                $("#ddlYear").append('<option value="-1">--Select--</option>');
                $.each(data.Table3, function (index, Value) {
                    $("#ddlYear").append("<option value='" + Value.Years + "'>" + Value.Years + "</option>");
                });
            }
        });
    }
    function SaveQuestion(rDD_Question) {
        $('.loader1').show();
        var rDD_Question = rDD_Question;
        $.ajax({
            type: "POST",
            url: '@Url.Action("SaveAppraisalQuestion", "AddAppraisalQuestion")',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({ rDD_Question: rDD_Question }),
            dataType: "json",
            success: function (data) {                
                $('.loader1').hide();
                if (data[0].Outtf == true) {
                    RedDotAlert_Success(data[0].Responsemsg);
                    //GetCategoryWiseEmployeeDetails();
                    $("#addRowModal").modal('hide');
                    $("#btnUpdateEmpDetails").text("Save");
                    $("#btnUpdateEmpDetails").attr("id", "btnSaveEmpDetails");
                    GetQuestionDetails();
                    GetCategoryList();
                }
                else {
                    RedDotAlert_Error(data[0].Responsemsg);
                }
            }
        });
    }
    function GetPreviousPeriodQuestion(PrevPeriod, CategoryId) {
        $('.loader1').show();
        var Html1 = "";
        $("#add-rowPrevious tbody").empty();
        Html1 += '<tr>';
        Html1 += '<td colspan="6" style="text-align:center">Please Wait....</td>';
        Html1 += '</tr>';
        $("#add-rowPrevious").show();
        $("#add-rowPrevious tbody").append(Html1);
        $.ajax({
            type: "POST",
            url: '@Url.Action("GetPreviousPeriodQuestion", "AddAppraisalQuestion")',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({ PrevPeriod: PrevPeriod, CategoryId: CategoryId }),
            dataType: "json",
            success: function (data) {
                debugger               
                $("#add-rowPrevious tbody").empty();                
                var Html = "";
                var a = data.Table;
                if (a.length > 0) {
                    $.each(a, function (Index, Value) {
                        Html += '<tr>';
                        Html += '<td>' + '<input type="text" class="form-control appqstns" value="' + Value.Question + '">' + '</td>';
                        Html += '<td><div class="form-button-action"><button type="button" data-toggle="tooltip" title="" class="btn btn-link btn-danger btnDel" data-original-title="Remove"><i class="fa fa-times"></i></button></div></td>';
                        Html += '</tr>';
                    });
                }
                else {
                    Html += '<tr>';
                    Html += '<td colspan="6" style="text-align:center">No Data Found</td>';
                    Html += '</tr>';
                }
                $("#add-rowPrevious tbody").append(Html);
                
                $('.loader1').hide();
            }
        });
    }

    function GetQuestionDetails() {        
        $("#tblShowQuestion tbody").empty();
        $("#tblShowQuestion tbody").append('<tr><td colspan="6" style="text-align:center">Please Wait....</td></tr>');
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetQuestionDetails", "AddAppraisalQuestion")',
            dataType: "json",
            success: function (data) {               
                
                $("#ddlSelectYear").empty();
                $("#ddlSelectYear").append('<option value="-1">--Select--</option>');
                if (data.Table3.length > 0) {
                    $.each(data.Table3, function (index, Value) {                        
                        $("#ddlSelectYear").append("<option value='" + Value.AllYear + "'>" + Value.AllYear + "</option>");                        
                    });
                    if (data.Table4.length > 0) {
                        $("#ddlSelectYear").val(data.Table4[0].CurrentYear);
                    }
                }            
                $("#myTab").empty();
                $.each(data.Table, function (Index, Value) {
                    var str1 = "";
                    str1 += '<li class="nav-item waves-effect waves-light"><a class="nav-link selectCategory" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="false" data="' + Value.CategoryId + '">' + Value.CategoryName + '</a></li >';
                    $("#myTab").append(str1);
                });
                $("#myTab li:eq(0) a").tab('show');
                var str2 = "";
                //$('#tblShowQuestion').dataTable().fnDestroy();
                $("#tblShowQuestion tbody").empty();
                if (data.Table1.length > 0) {
                    $.each(data.Table1, function (Index, Value) {                        
                        str2 += '<tr>';          
                        str2 += '<td style="display:none">' + Value.QuestionTransId + '</td>';
                        str2 += '<td>' + Value.Question + '</td>';                        
                        str2 += '<td><div class="form-button-action"><button type="button" id="btnEditCategory" title="" class= "btn btn-link btn-primary btn-lg edit dis" data-original-title="Edit Task"><i class="fa fa-edit"></i></button> <button type="button" id="" title="" class= "btn btn-link btn-success update dis" data-original-title="Edit Task"><i class="fas fa-save"></i></button> <button type="button" data-toggle="tooltip" data="' + Value.QuestionId + '" title="" class="btn btn-link btn-danger delete dis" data-original-title="Remove"><i class="fa fa-times"></i></button></div></td>';
                        str2 += '</tr>';                        
                    });
                    $("#btnLaunchAppraisal").show();
                }
                else {
                    $("#btnLaunchAppraisal").hide();
                    str2 += '<tr>';
                    str2 += '<td colspan="6" style="text-align:center">Question Not Assigned Under This Category</td>';
                    str2 += '</tr>';
                }
                $("#tblShowQuestion tbody").append(str2);
                $(".update").attr("hidden", "hidden");
                if (data.Table2.length > 0) {
                    var LaunchStatus = data.Table2[0].AppraisalLaunchStatus;
                }
                if (LaunchStatus == true) {
                    $('.dis').prop("disabled", true);
                    $('.dis').css("cursor", "not-allowed");
                    
                }
                else {
                    $('.dis').prop("disabled", false);
                    $('.dis').css("cursor", "");
                }
                $('.loader1').hide();                
            }
        });
    }
    
    $(document).ready(function () {
        $('.loader1').show();
        GetQuestionDetails();         
        GetCategoryList();
        $("#add-rowExcel").hide();
        $("#add-rowPrevious").hide();
    });

    function LaunchAppraisal() {     
        var Qperiod = $("#ddlSelectYear option:selected").val();
        const swalWithBootstrapButtons = Swal.mixin({
            confirmButtonClass: 'btn btn-success',
            cancelButtonClass: 'btn btn-danger',
            buttonsStyling: false,
        })
        swalWithBootstrapButtons.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            type: 'warning',
            showCancelButton: true,
            confirmButtonText: 'Yes, Launch it!',
            cancelButtonText: 'No, cancel!',
            reverseButtons: true
        }).then((result) => {            
            if (result.value) {
                $('.loader1').show();
                $.getJSON("/AddAppraisalQuestion/LaunchAppraisal", { "Qperiod": Qperiod }).done(function (data) {
                    $('.loader1').hide();
                    if (data[0].Outtf == true) {
                        RedDotAlert_Success(data[0].Responsemsg);
                        GetQuestionDetails();
                        GetCategoryList();
                    }
                    else {
                        RedDotAlert_Error(data[0].Responsemsg);
                    }
                });
            }
            else if (
                // Read more about handling dismissals
                result.dismiss === Swal.DismissReason.cancel
            ) {
                swalWithBootstrapButtons.fire(
                    'Cancelled',
                    'Your Record  is safe :)',
                    'error'
                )
            }
            $('.loader1').hide();
        });
    }

    function ClearModalData() {        
        GetCategoryList();
        $("#add-rowPrevious").hide();
        $("#add-rowExcel").hide();
        $("#add-rowPrevious tbody").empty();
        $("#add-rowExcel tbody").empty();
        $('.txtQuestion').each(function () {
            $(this).val("");            
            $(this).remove();
            $('.remove-me').remove();
            $('.divTypeQuestions').text("");
        });
        $("#ddlCopyFromPrevious").removeAttr("disabled", true);
        $("#action_name").removeAttr("disabled", true);
        $("#add-more").removeAttr("disabled", true);
        $("#action_name").css("cursor", "");
        $("#ddlCopyFromPrevious").css("cursor", "");
        $("#add-more").css("cursor", "");
        $("#action_json").removeAttr("disabled", true);        
        $("#action_json").css("cursor", "");
        $("#action_json").val("");
        $("#field").empty();
        $("#field").append('<div id="field0"><div class="form-group row"><label class="col-md-2 control-label divTypeQuestions" for="action_name" id="">Type Question :</label><div class="col-md-10" id="divTypeQuestion"><textarea id="action_name" name="action_name" type="text" placeholder="" class="form-control input-md txtQuestion"></textarea></div></div></div >');
        //$("#divTypeQuestion").empty();
        //$('.divTypeQuestions').text("Type Question:");
        //$("#divTypeQuestion").append('<textarea id="action_name" name="action_name" type="text" placeholder="" class="form-control input-md txtQuestion"></textarea>');
        $("#divAddMore").empty();
        $("#divAddMore").append('<button id="add-more" name="add-more" class="btn btn-primary btn-round">Add More</button>');
        var next = 0;
        $("#add-more").click(function (e) {            
            e.preventDefault();
            var addto = "#field" + next;
            var addRemove = "#field" + (next);
            next = next + 1;
            var newIn = ' <div id="field' + next + '" name="field' + next + '"><!-- Text input--><div class="form-group row"><label class="col-md-2 control-label divTypeQuestions" for="action_name">Type Question :</label><div class="col-md-10"><textarea id="action_name" name="action_name" type="text" placeholder="" class="form-control input-md txtQuestion"></textarea></div></div></div>';
            var newInput = $(newIn);
            var removeBtn = '<div class="col-12" style="overflow:hidden;"><button id="remove' + (next - 1) + '" class="btn btn-danger btn-round remove-me" >Remove</button></div></div><div id="field">';
            var removeButton = $(removeBtn);
            $(addto).after(newInput);
            $(addRemove).after(removeButton);
            $("#field" + next).attr('data-source', $(addto).attr('data-source'));
            $("#count").val(next);
            $('.remove-me').click(function (e) {
                e.preventDefault();
                var fieldNum = this.id.charAt(this.id.length - 1);
                var fieldID = "#field" + fieldNum;
                $(this).remove();
                $(fieldID).remove();
            });
        });
    }

