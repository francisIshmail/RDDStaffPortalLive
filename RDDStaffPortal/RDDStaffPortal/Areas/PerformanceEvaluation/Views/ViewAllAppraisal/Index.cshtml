
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_CommonLayout.cshtml";
}
<link href="~/Content/extra-style.css" rel="stylesheet" />
<style>
    tbody {
        width: 900px; /* Just for the demo          */
        overflow-y: hidden; /* Trigger vertical scroll    */
        overflow-x: auto; /* Hide the horizontal scroll */
    }
</style>
<input type="hidden" id="hdnEmpId" />
<input type="hidden" id="hdnRatingNo" />

<div class="main-panel">
    <div class="container">
        <div class="page-inner">
            <div class="row">
                <div class="col-md-12">
                    <div class="page-header mb-1">
                        <h4 class="page-title"><button class="p-0 border-transparent" hidden="hidden" style="font-size:17px" onclick="BackToEmployeeDetails();" id="btnBackEmpDetails"><i class="fas fa-chevron-circle-left pr-1"></i>Back</button> </h4>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            <div class="d-flex align-items-center">
                                <h4 class="card-title redtitle">All Employee's Performance Report</h4>
                                <div class="form-group ml-auto" id="">
                                    <label>Period-Year</label>
                                    <select id="ddlSelectYear" class="form-control">
                                        <option value="-1">---Select---</option>
                                    </select>
                                    <select id="ddlSelectYearEmp" class="form-control" hidden="hidden">
                                        <option value="-1">---Select---</option>
                                    </select>
                                </div>
                            </div>
                            <div class="row mx-3 mt-3"  id="divProfileDetails">
                                @*<div class="col-1" id="divProfPic">
                                    <img src="data:image;base64,@HttpContext.Current.Session["LoginName"]" alt="..." class="avatar-img rounded-circle">
                                </div>*@
                                <div class="col-11 p-0">
                                    <h2 id="headEmpName"></h2>
                                    <div class="row">
                                        <div class="col-3 p-0 emp" style="font-size:16px" id="divDepartment"></div>
                                        <div class="col-3 p-0 emp" style="font-size:16px" id="divCountry"></div>
                                        <div class="col-3 p-0 emp" style="font-size:16px" id="divYear"></div>
                                        <div class="col-3 p-0 emp" style="font-size:16px" id="divPeriod"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="loader1"></div>
                        <div class="card-body">
                            <section id="secEmpDetails">
                                <div class="col-md-4">
                                    <input id="myInput" type="text" placeholder="Search.." class="form-control">
                                </div>
                                <br />
                                <table class="display table table-striped table-hover" id="tblShowEmployee">
                                    <thead style="background-color: rgba(0,0,0,.05);">
                                        <tr style="font-size:14px">
                                            <th width="5%">#</th>
                                            <th width="30%">Employee</th>
                                            <th width="30%">Employee Submission Status</th>
                                            <th width="30%">Manager Submission Status</th>
                                            <th width="30%">PDF</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tblShowEmployeetbody">
                                        <tr>
                                            <td colspan="6" style="text-align:center">Please Wait...</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </section>
                            <section id="secEmpRating"></section>
                        </div>                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-3.5.1.min.js"></script>
<script src="~/Scripts/RedDotUtility.js"></script>

<script type="text/javascript">
    function GetEmployeeDetailsForHR() {
        $('.loader1').show();
        $("#divProfPic").show();
        $("#secEmpRating").empty();
        $("#secEmpDetails").show();
        $("#headEmpName").empty();
        $('.emp').empty();        
        $("#divSelectPeriodYear").show();
        $.ajax({
            type: "GET",
            url: '@Url.Action("GetDetailsForHR", "ViewAllAppraisal")',
            dataType: "json",
            success: function (data) { 
                debugger
                //var str = "";
                //str += data.Table[0].EmployeeName;
                //str += '<span class="badge badge-danger mb-1 ml-2">' + data.Table[0].DesigName + '</span>';
                //$("#headEmpName").append(str);
                $("#btnBackEmpDetails").attr("hidden", "hidden"); 
                $("#ddlSelectYear").empty();
                $("#ddlSelectYear").append('<option value="-1">--Select--</option>');
                if (data.Table1.length > 0) {
                    $.each(data.Table1, function (index, value) {
                        $("#ddlSelectYear").append('<option value="' + value.AllYear + '">' + value.AllYear + '</option>');
                    });
                    if (data.Table2.length > 0) {
                        $("#ddlSelectYear").val(data.Table2[0].CurrentYears);
                    }
                }
                $("#ddlSelectYearEmp").empty();
                $("#ddlSelectYearEmp").append('<option value="-1">--Select--</option>');
                if (data.Table1.length > 0) {
                    $.each(data.Table1, function (index, value) {
                        $("#ddlSelectYearEmp").append('<option value="' + value.AllYear + '">' + value.AllYear + '</option>');
                    });
                    if (data.Table2.length > 0) {
                        $("#ddlSelectYearEmp").val(data.Table2[0].CurrentYears);
                    }
                }                
                $("#tblShowEmployee tbody").empty();
                var str = "";
                var Slno = 1;
                var d = data.Table;
                if (d.length > 0) {
                    $.each(d, function (Index, Value) {
                        str += '<tr>';
                        str += '<th scope="row">' + Slno + '</th>';
                        str += '<td style="display:none">' + Value.EmployeeId + '</td>';
                        str += '<td style="font-size:16px"><a href="#" target="" class="emprating" data-empid=' + Value.EmployeeId + '>' + Value.EmployeeName + '</a></td>';
                        if (Value.EmpStatus == "Completed") {
                            str += '<td><span class="badge badge-success">Completed</span></td>';
                        }
                        else {
                            str += '<td><span class="badge badge-warning">Pending</span></td>';
                        }
                        if (Value.MngStatus == "Completed") {
                            str += '<td><span class="badge badge-success">Completed</span></td>';
                        }
                        else {
                            str += '<td><span class="badge badge-warning">Pending</span></td>';
                        }
                        str +='<td><i class="fa fa-download downloadPdf"></i></td>'
                        str += '</tr>';
                        Slno++;
                    });
                }
                else {
                    str += '<tr>';
                    str += '<td colspan="6" style="text-align:center">No Employee Filled Their Appraisal Form</td>';
                    str += '</tr>';
                }                       
                $("#tblShowEmployee tbody").append(str);
                $('.loader1').hide();
            }
        });
        $("#secEmpRating").load('@Url.Action("GetPartial", "ViewAllAppraisal")');
        //$("#myTab").append('<div>Hi pratim</div>');
        //$("#secEmpRating").append('<div>Hi pratim</div>')
        $("#secEmpRating").hide();        
    }
    $(document).ready(function () {
        $("#divProfileDetails").hide();
        GetEmployeeDetailsForHR();
        $("#secEmpRating").load('@Url.Action("GetPartial", "ViewAllAppraisal")');
        $("#secEmpRating").hide(); 
        $("#myInput").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#tblShowEmployeetbody tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
    $(document).on('click', '.downloadPdf', function () {
        var EmpId = $(this).closest('tr').find('td:eq(0)').html();
        var PeriodYear = $("#ddlSelectYear option:selected").val();
        var UrlId = EmpId + "$" + PeriodYear;
        window.location.href = '/PerformanceEvaluation/ViewAllAppraisal/GeneratePDF?UrlId=' + UrlId + '';
    });
    $(document).on('change', '#ddlSelectYear', function () {
        $('.loader1').show();        
        var Qperiod = $(this).val();
        if (Qperiod == "-1") {            
            $("#tblShowEmployee tbody").empty();
            var str1 = "";
            str1 += '<tr>';
            str1 += '<td colspan="6" style="text-align:center">No Employee Filled Their Appraisal Form For This Period</td>';
            str1 += '</tr>';
            $("#tblShowEmployee tbody").append(str1);
            return false;
        }
        $.ajax({
            type: "POST",
            url: '@Url.Action("GetEmployeeDetailsOnPeriod", "ViewAllAppraisal")',
            data: { "Qperiod": Qperiod },
            dataType: "json",
            success: function (data) {
                $("#tblShowEmployee tbody").empty();
                var str = "";
                var Slno = 1;
                var d = data.Table;
                if (d.length > 0) {
                    $.each(d, function (Index, Value) {
                        str += '<tr>';
                        str += '<th scope="row">' + Slno + '</th>';
                        str += '<td style="display:none">' + Value.EmployeeId + '</td>';
                        str += '<td style="font-size:16px"><a href="#" target="" class="emprating" data-empid=' + Value.EmployeeId + '>' + Value.EmployeeName + '</a></td>';
                        if (Value.EmpStatus == "Completed") {
                            str += '<td><span class="badge badge-success">Completed</span></td>';
                        }
                        else {
                            str += '<td><span class="badge badge-warning">Pending</span></td>';
                        }
                        if (Value.MngStatus == "Completed") {
                            str += '<td><span class="badge badge-success">Completed</span></td>';
                        }
                        else {
                            str += '<td><span class="badge badge-warning">Pending</span></td>';
                        }
                        str += '<td><i class="fa fa-download downloadPdf"></i></td>'
                        str += '</tr>';
                        Slno++;
                    });
                }
                else {
                    str += '<tr>';
                    str += '<td colspan="6" style="text-align:center">No Employee Filled Their Appraisal Form</td>';
                    str += '</tr>';
                }
                $("#tblShowEmployee tbody").append(str);
                $('.loader1').hide();
            }
        });
    });
