                    $("#btnPdfUpload").css("cursor", "not-allowed");
                    $("#btnDownloadSignedPdf").attr("disabled", "disabled");
                    $("#btnDownloadSignedPdf").css("cursor", "not-allowed");
                }
                else{
                    $("#AttachmentUrl").removeAttr("disabled", "disabled");
                    $("#AttachmentUrl").css("cursor", "");
                    $("#btnPdfUpload").removeAttr("disabled", "disabled");
                    $("#btnPdfUpload").css("cursor", "");
                    $("#btnDownloadSignedPdf").attr("disabled", "disabled");
                    $("#btnDownloadSignedPdf").css("cursor", "not-allowed");
                }
                $('.loader1').hide();
            }
        });
    });
</script>

