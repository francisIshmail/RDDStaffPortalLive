    function SaveEmpRating(rDD_EmpAppraisal) {
        $('.loader1').show();
        var rDD_EmpAppraisal = rDD_EmpAppraisal;
        $.ajax({
            type: "POST",
            url: '@Url.Action("SaveEmployeeRating", "EmployeeRating")',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({ rDD_EmpAppraisal: rDD_EmpAppraisal }),
            dataType: "json",
            success: function (data) {
                debugger
                $('.loader1').hide();
                if (data[0].Outtf == true) {
                    RedDotAlert_Success(data[0].Responsemsg);
                    //GetCategoryWiseEmployeeDetails();
                    GetEmployeeDetails();
                }
                else {
                    RedDotAlert_Error(data[0].Responsemsg);
                }
            }
        });
    }

    function SaveFinalForm() {
        //$('.loader1').show();
        var EmployeeId = $.trim($("#hdnEmployeeId").val());
        var Year= $.trim($("#divYear").text());
        var Period = $.trim($("#divPeriod").text());
        const swalWithBootstrapButtons = Swal.mixin({
            confirmButtonClass: 'btn btn-success',
            cancelButtonClass: 'btn btn-danger',
            buttonsStyling: false,
        })
        swalWithBootstrapButtons.fire({
            title: 'Have you answered all the questions of all categories?',
            text: "You won't be able to revert this!",
            type: 'warning',
            showCancelButton: true,
            confirmButtonText: 'Yes, Submit it!',
            cancelButtonText: 'No, cancel!',
            reverseButtons: true
        }).then((result) => {
            $('.loader1').show();
            if (result.value) {
                $.getJSON("/FinalSaveEmployeeRating", { "EmployeeId": EmployeeId, "Year": Year,"Period":Period }).done(function (data) {
                    $('.loader1').hide();
                    if (data[0].Outtf == true) {
                        RedDotAlert_Success(data[0].Responsemsg);
                        GetEmployeeDetails();
                    }
                    else {
                        RedDotAlert_Error(data[0].Responsemsg);
                    }
                });
            }
            else if (
                // Read more about handling dismissals
                result.dismiss === Swal.DismissReason.cancel
            ) {
                swalWithBootstrapButtons.fire(
                    'Cancelled',
                    'Your Record  is safe :)',
                    'error'
                )
            }
            $('.loader1').hide();
        });
    }
        
    $("#AttachmentUrl").on("change", function () {
        var data = new FormData();
        var files = $("input[id = 'AttachmentUrl']").get(0).files;
        var val = $(this).val();
        switch (val.substring(val.lastIndexOf('.') + 1).toLowerCase()) {
            case 'gif': case 'jpg': case 'png': case 'pdf':
                break;
            default:
                $(this).val('');
                $("input[id='AttachmentUrl'][type='hidden']").val('');
                // error message here
                RedDotAlert_Error("Upload only pdf");
                return;
                break;
        }
        if (files.length > 0) {
            data.append("Files", files[0]);
        }

        $.ajax({
            url: '@Url.Action("UploadPDF", "EmployeeRating")',
            type: "POST",
            processData: false,
            contentType: false,
            data: data,
            success: function (response) {
                if (response.indexOf("Error occurred") == 0) {
                    $("input[id='AttachmentUrl'][type='hidden']").val('');
                    RedDotAlert_Error(response);
                }
                else {
                    $("input[id='AttachmentUrl'][type='hidden']").val(response);
                    //alert(response);
                }
            },
            error: function (er) {
                RedDotAlert_Error(er);
            }
        });
    });

    $(document).on('click', '#btnPdfUpload', function () {        
        var valrrr = $("#AttachmentUrl[type='hidden']").val();       
        if (valrrr == "") {
            RedDotAlert_Error("Please upload signed pdf");
            return false;
        }
        var rDD_AppraisalPdfUpload = {
            AttachmentUrl: $("#AttachmentUrl[type='hidden']").val(),
            EmployeeId: $.trim($("#hdnEmployeeId").val()),
            Year: $.trim($("#divYear").text()),
            Period: $.trim($("#divPeriod").text())
        };
        $('.loader1').show();
        //var rDD_EmpAppraisal = rDD_EmpAppraisal;
        $.ajax({
            type: "POST",
            url: '@Url.Action("SavePdf", "EmployeeRating")',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify({ rDD_AppraisalPdfUpload: rDD_AppraisalPdfUpload }),
            dataType: "json",
            success: function (data) {
                debugger
                $('.loader1').hide();
                if (data[0].Outtf == true) {
                    RedDotAlert_Success(data[0].Responsemsg);                    
                    GetEmployeeDetails();                  
                    
                }
                else {
                    RedDotAlert_Error(data[0].Responsemsg);
                }
            }
        });

    });
