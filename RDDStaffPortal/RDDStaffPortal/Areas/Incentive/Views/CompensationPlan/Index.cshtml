<script type="text/javascript">

    var tblhead1 = ['CompPlanId', 'DesigId', 'DesigName', 'EmployeeName', 'Period', 'Years', 'Currency', 'TotalCompensation', 'TotalSplitRevPercent', 'TotalSplitGpPercent', 'TargetNumber', 'AcceptedBySalesperson', 'CreatedOn', 'CreatedBy', 'LastUpdatedOn', 'LastUpdatedBy'];
    var tblhide = [];
    var tblhead2 = [];
    var dateCond = ['CreatedOn', 'LastUpdatedOn'];
    var arr = [];
    var curPage = 1;
    var me = getUrlVars()["CompPlanid"];
    if (me != undefined) {
        //$("#btnAction").show();
        GetMode(me);
    } else {
        //Getdata();
    }

    function viewmode(CompPlanid) {
        $.post("/VIEWRDDCOMPPLAN", { CompPlanId: CompPlanid }, function (response) {
            $("#tblid").hide();
            $("#idCard").html(response);
            RedDot_Button_New_HideShow();
            debugger
            $(".txtcheck").each(function (index) {
                if ($("#" + $(this).attr("id") + "").val() !== '') {
                    $("#div-" + $(this).attr("id") + "").removeClass('has-error1').addClass('has-success1');
                }
            });
            $(".dropcheck").each(function (index) {
                if ($("#" + $(this).attr("id") + "").val() !== '0') {
                    $("#div-" + $(this).attr("id") + "").removeClass('has-error1').addClass('has-success1');
                }
            });

            $("#btnSave").show();
            //$("#btnDelete").hide();
            $("#btnCancel").hide();

        })
    }
    function Editmode(CompPlanid) {
        $.post("/ADDCOMPPLAN", { TEMPId: CompPlanid }, function (response) {
            $("#tblid").hide();
            $("#idCard").html(response);
            RedDot_Button_New_HideShow();
            debugger

            $("#btnSave").show();
            //$("#btnDelete").hide();
            $("#btnCancel").hide();

        })
    }

    function GetMode(CompPlanid) {
        var CompPlanid = CompPlanid;
        $.post("/GETMODE", { CompPlanid: CompPlanid }, function (response) {
            debugger
            if (response.Table[0].UserType == "Employee") {
                viewmode(CompPlanid);
                return;
            }
            if (response.Table[0].UserType == "Manager") {
                Editmode(CompPlanid);
                return;
            }
            if (response.Table[0].UserType == "Invalid") {
                RedDotAlert_Error("Invalid User");
                return;
            }
        });
    }


    //#region Pageload table bind*/
    function GetCompensationData() {
        var value1 = $("#Search-Forms").val().toLowerCase();
        $('.loader1').show();
        var data = JSON.stringify({
            pagesize: 50,
            pageno: curPage,
            psearch: value1,
        });
        arr = RedDot_DivTable_Fill("III", "/GETCOMPENSATION", data, dateCond, tblhead1, tblhide, tblhead2);
    }
    //#endregion
    //#region Next Button*/
    $('.next').bind('click', function (e) {
        e.preventDefault();
        $(".loader1").show();
        if (arr.data[0].TotalCount < 50) {
            $(".loader1").hide();
            return;
        }
        curPage++;
        var value1 = $("#Search-Forms").val().toLowerCase();
        if (curPage > arr.data[0].TotalCount)
            curPage = 0;
        var data = JSON.stringify({
            pagesize: 50,
            pageno: curPage,
            psearch: value1,
        });
        arr = RedDot_DivTable_Fill("III", "/GETCOMPENSATION", data, dateCond, tblhead1, tblhide, tblhead2);
    });
    //#endregion
    //#region Prev Button*/
    $('.prev').bind('click', function (e) {
        e.preventDefault();
        $(".loader1").show();
        var value1 = $("#Search-Forms").val().toLowerCase();
        if (arr.data[0].TotalCount < 50) {
            $(".loader1").hide();
            return;
        }
        curPage--;

        if (curPage < 0)
            curPage = (arr.data[0].TotalCount - 1);
        var data = JSON.stringify({
            pagesize: 50,
            pageno: curPage,
            psearch: value1,
        });
        arr = RedDot_DivTable_Fill("III", "/GETCOMPENSATION", data, dateCond, tblhead1, tblhide, tblhead2);
    });
    //#endregion
    //#region Search Textbox*/
    $("#Search-Forms").on("keyup", function (e) {
        e.preventDefault();
        $(".loader1").show();
        var value1 = $(this).val().toLowerCase();
        var data = JSON.stringify({
            pagesize: 50,
            pageno: curPage,
            psearch: value1,
        });
        arr = RedDot_DivTable_Fill("III", "/GETCOMPENSATION", data, dateCond, tblhead1, tblhide, tblhead2);
    });
    //#endregion
    function GetSalesDetails(_Empid,_Period,_Year) {
        $.post("/GETSALEDETAILS", { EmployeeId: _Empid, Period: _Period, Years: _Year }, function (data) {

            var a = data;
            if (a.Table.length == 0) {
                RedDotAlert_Error("No Record Found");
                //$('div#Ist').not(':first').remove();
                //$("#Ist")[0].remove();
                return;
            }
            else {
                $('div#Ist').not(':first').remove();
                $("#txtSplitREVpercent").val("0");
                $("#txtSplitGPpercent").val("0");
                $.each(a.Table, function (index, value) {
                    var tr = $("#Ist").clone();
                    tr.find(".Abcd")[0].children[0].textContent = value.BU;
                    tr.find(".Abcd")[1].children[0].textContent = value.RevenueTarget;
                    tr.find(".Abcd")[2].children[0].textContent = value.GPTarget;
                    $("#Ibody").append(tr);
                });
                $("#Ist")[0].remove();
            }

        });
    }
    function GetKpiDetail(_Desigid, _Period, _Year) {
        $.post("/GETKPIDETAILS", { DesigId: _Desigid, Period: _Period, Years: _Year }, function (data) {

            var a = data;
            if (a.Table.length == 0) {
                $("#HdnRevSplit").val("");
                $("#HdnGpSplit").val("");

                //$('div#IIst').not(':first').remove();
                //$("#IIst")[0].remove();
                return;
            }
            else {
                $('div#IIst').not(':first').remove();

                $.each(a.Table, function (index, value) {
                    var tr = $("#IIst").clone();
                    tr.find(".Abcd")[0].children[0].textContent = value.KPI_Parameter;
                    //tr.find(".Abcd")[1].children[0].textContent = value.GPTarget;
                    tr.find(".Abcd")[2].children[0].textContent = value.Split_Percentage;
                    $("#IIbody").append(tr);
                });
                $("#IIst")[0].remove();
                $("#IIst")[0].remove();
                $("#IIst")[0].remove();
                //var d = data.Table1[0].Split_Percentage;
                //var d1 = data.Table2[0].Split_Percentage;
                //$("#HdnRevSplit").val(d);
                //$("#HdnGpSplit").val(d1);
            }
        });
    }

    function GetKpiTargets(_Desigid, _Period, _Year) {
        $.post("/GETKPITARGET", { DesigId: _Desigid, Period: _Period, Years: _Year }, function (data) {

            var a = data;
            if (a.Table.length == 0) {
                $("#HdnRevSplit").val("");
                $("#HdnGpSplit").val("");
                //$('div#IIst').not(':first').remove();
                //$("#IIst")[0].remove();
                return;
            }
            else {
                var d = data.Table[0].Split_Percentage;
                var d1 = data.Table1[0].Split_Percentage;
                $("#HdnRevSplit").val(d);
                $("#HdnGpSplit").val(d1);
            }
        });
    }

    function GetLoginUserId() {
        $.get("/GETLOGINUSERID", function (data) {
            $("#hdnLoginUserId").val(data.EmployeeId);
            var Empid = $("#hdnLoginUserId").val();
            GetLoginUserEmail(Empid);
        });
    }

    function GetLoginUserEmail(Empid) {
        $.post("/GETLOGINUSERMAIL", { Empid: Empid }, function (data) {
            debugger
            $("#hdnLoginUserMail").val(data.Table[0].Email);
            //alert($("#hdnLoginUserMail").val());
        });
    }

    function GetEmployeeStatus(Empid) {
        $.post("/GETEMPSTATUS", { }, function (response) {
            debugger
            $("#hdnLoginEmployeeStatus").val(response.Table[0].UserType);           
        });
    }

    $(document).ready(function () {
        GetLoginUserId();
        //GetEmployeeStatus();        
        GetCompensationData();
        $('.loader1').show();
        //GetKpiData();
        $('.loader1').hide();
        $("#divKPIshow").show();
        //GetKPIdetails();
        $("#btnSave").hide();
        $("#btnCancel").hide();
    });
    $(document).on('click', '#btnAdd', function (e) {
        e.preventDefault();
        $.post("/ADDCOMPPLAN", { TEMPId: -1 }, function (response) {
            $("#idCard").html(response);
            RedDot_Button_New_HideShow();
        })
    });
    $(document).on('click', '#btnCancel', function (e) {
        e.preventDefault();
        RedDot_Button_Init_HideShow();
        $("#idCard").html("");
        $("#divKPIshow").show();
        $("#ContantsPayment").hide();
        $("#btnNew").show();
        $("#btnSave").hide();
