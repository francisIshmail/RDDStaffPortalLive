    $(document).on('change', '#EmployeeId', function () {
        var Empide = $(this).val();
        $.post("/GETDESIGNATION", { Empide: Empide }, function (data) {

            $("#DesigName").val(data.DesigName);
            $("#DesigId").val(data.DesigId);
            $("#Email").val(data.Email);
            //alert($("#Email").val());
            //$("#HdnCCMail").val("pratim.d@reddotdistribution.com");
        });
        var _Empid = $(this).val();
        var _Desigid = $("#DesigId").val();
        var _Period = $("#Period option:selected").val();
        var _Year = $("#Years option:selected").val();
        if (_Empid == 0 || _Empid == undefined || _Period == 0 || _Period == undefined || _Year == 0 || _Year == undefined) {
            return;
        }
        else {
            GetKpiDetail(_Desigid, _Period, _Year);
            GetSalesDetails(_Empid, _Period, _Year);
            GetKpiTargets(_Desigid, _Period, _Year);
        }
    });

    $(document).on('change', '#Period', function () {
        var _Empid = $("#EmployeeId option:selected").val();
        var _Desigid = $("#DesigId").val();
        var _Period = $(this).val();
        var _Year = $("#Years option:selected").val();
        if (_Empid == 0 || _Empid == undefined || _Period == 0 || _Period == undefined || _Year == 0 || _Year == undefined) {
            return;
        }
        else {
            GetKpiDetail(_Desigid, _Period, _Year);
            GetSalesDetails(_Empid, _Period, _Year);
            GetKpiTargets(_Desigid, _Period, _Year);
            //GetSalesRevAndGpSplitPercent(_Desigid, _Period, _Year);
        }
        //var Obj = JSON.stringify({ EmployeeId: _Empid, Period: _Period, Year: _Year });

    });
    $(document).on('change', '#Years', function () {
        var _Empid = $("#EmployeeId option:selected").val();
        var _Desigid = $("#DesigId").val();
        var _Period = $("#Period option:selected").val();
        var _Year = $(this).val();
        if (_Empid == 0 || _Empid == undefined || _Period == 0 || _Period == undefined || _Year == 0 || _Year == undefined) {
            return;
        }
        else {
            GetKpiDetail(_Desigid, _Period, _Year);
            GetSalesDetails(_Empid, _Period, _Year);
            GetKpiTargets(_Desigid, _Period, _Year);
            //GetSalesRevAndGpSplitPercent(_Desigid, _Period, _Year);
        }
        //var Obj = JSON.stringify({ EmployeeId: _Empid, Period: _Period, Year: _Year });

    });

    $(document).on('keyup', '.text', function () {
        var Sum = 0;
        var exSum1;
        var p = $(this).val();
        var GpSplit = $("#HdnGpSplit").val();
        if (parseFloat(GpSplit) < parseFloat(p)) {
            $(this).val("0");
        }
        $(".OriginatorDet").each(function () {
            var SplitGpPercent = $(this).find("[name^='txtSplitGPpercent']").val() || 0;
            Sum = Sum + parseFloat(SplitGpPercent);
            $("#txtTotalGP").val(Sum);
            exSum1 = Sum - GpSplit;
        });
        if (Sum > GpSplit) {
            var p1 = parseFloat(Sum) - parseFloat(p);
            var d = GpSplit - p1;
            $(this).val(GpSplit - p1);
            RedDotAlert_Error("Sum cannot exceed" + " " + GpSplit);
            var finsum1 = Sum - exSum1;
            $("#txtTotalGP").val(finsum1);
        }
    });
    $(document).on('keyup', '.text1', function () {
        //var Sum = 0;
        var Sum1 = 0;
        var exSum;
        var t = $(this).val() || 0;
        var RevSplit = $("#HdnRevSplit").val();
        if (t > RevSplit) {
            $(this).val("0");
        }
        $(".OriginatorDet").each(function () {
            var SplitRevPercent = $(this).find("[name^='txtSplitREVpercent']").val() || 0;
            //Sum = Sum + parseFloat(SplitRevPercent);
            Sum1 = Sum1 + parseFloat(SplitRevPercent);
            $("#txtTotalREV").val(Sum1);
            exSum = Sum1 - RevSplit;
        });
        if (Sum1 > RevSplit) {
            var t1 = parseFloat(Sum1) - parseFloat(t);
            var d = RevSplit - t1;
            $(this).val(RevSplit - t1);
            RedDotAlert_Error("Sum cannot exceed" + " " + RevSplit);
            var finsum = Sum1 - exSum;
            $("#txtTotalREV").val(finsum);
        }
    });
    $(document).on('keyup', '.text2', function () {
        var Sum = 0;
        $(".OriginatorDet1").each(function () {
            var TargetNum = $(this).find("[name^='txtTargetNo']").val() || 0;
            Sum = Sum + parseFloat(TargetNum);
            $("#txtTotalTarget").val(Sum);
        });
    });
    $(document).on('click', '#btnSave', function () {
        var RDD_CompPlan = {
            CompPlanId: $("#CompPlanId").val(),
            Years: $("#Years option:selected").val(),
            Period: $("#Period option:selected").val(),
            EmployeeId: $("#EmployeeId option:selected").val(),
            EmployeeName: $("#EmployeeId option:selected").text(),
            Email: $("#Email").val(),
            DesigId: $("#DesigId").val(),
            DesigName: $("#DesigName").val(),
            Currency: $("#Currency option:selected").text(),
            TotalCompensation: $.trim($("#TotalCompensation").val()),
            EditFlag: $("#EditFlag").val(),

            TotalSplitGpPercent: $.trim($("#txtTotalGP").val()),
            TotalSplitRevPercent: $.trim($("#txtTotalREV").val()),
            TargetNumber: $.trim($("#txtTotalTarget").val()),
            AcceptedBySalesperson: $("#AcceptedBySalesperson option:selected").val(),
            Description: $.trim($("#Description").val()),
            RDD_BU_CompensationPlanList: [],
            RDD_KPI_CompensationPlanList:[]
        };
        $(".OriginatorDet").each(function () {
            var Currow = $(this).closest("#Ist");
            var BU = $.trim(Currow.find(".Abcd").eq(0).text());
            var RevTarget = $.trim(Currow.find(".Abcd").eq(1).text());
            var GpTarget = $.trim(Currow.find(".Abcd").eq(2).text());
            var SplitGpPercent = $.trim($(this).find("[name^='txtSplitGPpercent']").val());
            var SplitRevPercent = $.trim($(this).find("[name^='txtSplitREVpercent']").val());
            if (SplitGpPercent != '' && SplitRevPercent != '' && SplitGpPercent != undefined && SplitRevPercent!=undefined) {
                var RDD_BU_Compplan = {
                    BU: BU,
                    GPTarget: GpTarget,
                    RevenueTarget: RevTarget,
                    Rev_Split_Percentage: SplitRevPercent,
                    GP_Split_Percentage: SplitGpPercent
                };
                RDD_CompPlan.RDD_BU_CompensationPlanList.push(RDD_BU_Compplan);
            }
        });
        $(".OriginatorDet1").each(function () {
            var Currow = $(this).closest("#IIst");
            var KpiParameters = Currow.find(".Abcd").eq(0).text();
            var KpiTarget = $(this).find("[name^='txtTargetNo']").val();
            var SplitPercent = $.trim(Currow.find(".Abcd").eq(2).text());
            if (KpiTarget != '' && KpiTarget != undefined)  {
                var RDD_KPI_Compplan = {
                    KPI_Parameter: KpiParameters,
                    KPI_Target: KpiTarget,
                    KPI_Split_Percentage: SplitPercent
                };
                RDD_CompPlan.RDD_KPI_CompensationPlanList.push(RDD_KPI_Compplan);
            }
        });
        var ValidateFormCheck = ValidateForm(RDD_CompPlan);
        if (ValidateFormCheck.formValid == false) {
            RedDotAlert_Error(ValidateFormCheck.ErrorMessage);
            return;
        }
        var data = JSON.stringify({ RDD_CompPlan: RDD_CompPlan });

        $.ajax({
            type: "POST",
            url: '@Url.Action("SaveCompensationPlan", "CompensationPlan")',
            async: false,
            cache: false,
            data: data,
            dataType: "Json",
            contentType: "Application/json",
            success: function (data) {
                debugger
                if (data[0].Outtf == true) {
                    RedDotAlert_Success(data[0].Responsemsg);
                    var Compid = data[0].Id;
                    var MailFlag = $("#MailFlag").val();
                    var Email1 = $("#Email").val();
                    var Email2 = $("#hdnLoginUserMail").val();
                    var Acceptstatus = $("#AcceptedBySalesperson option:selected").val();
                    //window.location.href = '@Url.Action("Index", "CompensationPlan")';
                    window.location.href = "/Incentive/CompensationPlan/GeneratePdf?CompPlanid=" + data[0].Id + "&Email2=" + Email2 + "&MailFlag=" + MailFlag + "&Acceptstatus=" + Acceptstatus + "";
                }
                else {
                    RedDotAlert_Error(data[0].Responsemsg);
                }
            }
        });
    });

    $(document).on('click', '#btnPrint', function () {
