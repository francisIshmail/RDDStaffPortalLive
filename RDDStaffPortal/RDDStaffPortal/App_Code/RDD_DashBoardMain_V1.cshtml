            }
            else if (submenu1 == 3)
            {
                colcss1 = "col-sm-8 col-md-4";
            }
            else if (submenu1 == 2)
            {
                colcss1 = "col-sm-8 col-md-6";
            }
            else if (submenu1 == 1)
            {
                colcss1 = "col-sm-8 col-md-12";
